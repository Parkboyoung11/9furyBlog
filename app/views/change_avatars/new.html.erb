<%= form_for :change_avatar, html: {class: "form-signin"} do |f| %>
  <h1 class="h3 mb-3 font-weight-normal">Change your Avatar</h1>
  <%= f.text_field :avatar, placeholder: "Enter your avatar link", class: "form-control", required: true%>
  <h5></h5>
  <%= f.submit "Submit", class: "btn btn-lg btn-primary btn-block" %>
<% end %>
